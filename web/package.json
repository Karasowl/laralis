{
  "name": "laralis-web",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "kill-node": "taskkill /F /IM node.exe 2>nul || echo No hay procesos Node activos",
    "kill-next": "node scripts/kill-old-next.js",
    "dev": "node scripts/kill-old-next.js && next dev",
    "dev:wsl": "node scripts/kill-old-next.js && cross-env PORT=3001 WATCHPACK_POLLING=true CHOKIDAR_USEPOLLING=1 NEXT_WEBPACK_USEPOLLING=1 next dev -H 127.0.0.1 -p 3000",
    "dev:wsl:clean": "node scripts/kill-old-next.js && rm -rf .next && cross-env WATCHPACK_POLLING=true CHOKIDAR_USEPOLLING=1 NEXT_WEBPACK_USEPOLLING=1 next dev",
    "dev:force": "npm run kill-node & next dev",
    "dev:clean": "npm run kill-next && next dev",
    "dev:safe": "node scripts/cleanup.js & npm run kill-node && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:unit": "vitest run lib/calc",
    "test:integration": "vitest run tests/calc",
    "test:api": "vitest run tests/api",
    "test:multitenancy": "vitest run tests/api/multitenancy.test.ts",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:multitenancy": "cypress run --spec 'cypress/e2e/multi-tenancy.cy.ts'",
    "test:e2e:auth": "cypress run --spec 'cypress/e2e/01-auth.cy.ts'",
    "test:e2e:patients": "cypress run --spec 'cypress/e2e/02-patients.cy.ts'",
    "test:e2e:supplies": "cypress run --spec 'cypress/e2e/03-supplies.cy.ts'",
    "test:e2e:services": "cypress run --spec 'cypress/e2e/04-services.cy.ts'",
    "test:e2e:treatments": "cypress run --spec 'cypress/e2e/05-treatments.cy.ts'",
    "test:e2e:settings": "cypress run --spec 'cypress/e2e/06-fixed-costs-settings.cy.ts'",
    "test:e2e:marketing": "cypress run --spec 'cypress/e2e/07-marketing.cy.ts'",
    "test:all": "node test-runner.js all",
    "test:quick": "npm run test:unit && npm run test:integration",
    "test:full": "npm run test:unit && npm run test:integration && npm run test:api && npm run test:e2e",
    "dev:preview": "concurrently \"npm run dev\" \"echo 'Abre Browser Preview en VSCode con Ctrl+Shift+P'\"",
    "dev:mobile": "next dev -H 0.0.0.0",
    "typecheck": "tsc --noEmit",
    "i18n:check": "node scripts/check-i18n.mjs",
    "i18n:fill": "node scripts/fill-i18n-missing.mjs",
    "i18n:collect": "node scripts/collect-used-i18n.mjs",
    "i18n:scan": "node scripts/scan-i18n-usage.mjs",
    "i18n:lint": "node scripts/collect-used-i18n.mjs && node scripts/missing-used-i18n.mjs && node scripts/scan-i18n-usage.mjs",
    "i18n:fix-es": "node scripts/fix-es-encoding.mjs && node scripts/scan-i18n-usage.mjs"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.48.0",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cookies-next": "^4.3.0",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.469.0",
    "next": "^14.2.20",
    "next-intl": "^3.26.0",
    "next-themes": "^0.4.6",
    "otplib": "^12.0.1",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.62.0",
    "recharts": "^3.1.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.14.5",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^22.10.6",
    "@types/react": "^18.3.16",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.9",
    "autoprefixer": "^10.4.20",
    "cross-env": "^10.0.0",
    "cypress": "^14.5.4",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.20",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
