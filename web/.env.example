# Supabase Configuration
# Get these values from your Supabase project dashboard
# https://supabase.com/dashboard/projects
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Development Configuration
NODE_ENV=development
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_APP_VERSION=0.2.0
TOTP_ENCRYPTION_KEY=generate_a_64_character_random_secret

# AI Configuration
# Select providers: deepgram|whisper, kimi|openai|deepseek, deepgram|fishaudio|openai
AI_STT_PROVIDER=deepgram
AI_LLM_PROVIDER=kimi
AI_TTS_PROVIDER=deepgram    # Recommended: Uses same Deepgram API key as STT

# AI API Keys (get one based on your provider selection)
DEEPGRAM_API_KEY=your_deepgram_api_key_here    # Required for STT and TTS (if using deepgram for both)
KIMI_API_KEY=your_kimi_api_key_here

# Alternative providers (uncomment if using different providers)
# FISH_AUDIO_API_KEY=your_fish_audio_api_key_here    # Optional: Alternative TTS
# OPENAI_API_KEY=your_openai_api_key_here            # Optional: Fallback for STT, LLM, TTS
# DEEPSEEK_API_KEY=your_deepseek_api_key_here        # Optional: Budget LLM

# AI Configuration (optional)
AI_LLM_MODEL=kimi-k2-thinking    # Use K2 Thinking for better reasoning (default, slower but more accurate)
# AI_LLM_MODEL=moonshot-v1-32k   # Alternative: faster but less reasoning power
AI_DEFAULT_LANGUAGE=es
AI_LLM_TEMPERATURE=0.3
AI_RATE_LIMITING_ENABLED=true
AI_RATE_LIMIT_PER_HOUR=100
